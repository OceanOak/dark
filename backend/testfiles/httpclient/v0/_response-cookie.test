[expected-request]
GET PATH HTTP/1.1
Host: HOST
Content-Length: 0

[response]
HTTP/1.1 200 OK
Date: xxx, xx xxx xxxx xx:xx:xx xxx
Content-type: text/plain; charset=utf-8
Content-Length: LENGTH
Set-Cookie: yummy_cookie=choco
Set-Cookie: tasty_cookie=strawberry

Hello back

[test]
// This is to test whether the cookie ends up in entering the HttpClient "cookie
// jar", as presumably all other tests will fail if it does.
(let response = (Stdlib.HttpClient.request "get" "http://URL" [] []) |> Builtin.unwrap
 let respHeaders = response.headers |> Stdlib.List.filter (fun h -> Stdlib.Tuple2.first h != "date")
 {response with headers = respHeaders}) ==
   Stdlib.HttpClient.Response
    { statusCode = 200L
      headers =
        [
          ("server", "kestrel")
          ("Set-Cookie", "tasty_cookie=strawberry")
          ("content-length", "LENGTH")
          ("content-type", "text/plain; charset=utf-8")
        ]
      body = "Hello back" |> Stdlib.String.toBytes}
